@{
}

@model string

<div id="reciept-list">
    <div class="spinner-border" role="status">
        <span class="visually-hidden">Loading...</span>
    </div>
</div>


@section Scripts{
<script>
    $(document).ready(function () {
        LoadRecieptList();
    });
    
    function LoadRecieptList() {
        $.ajax({
            method: "GET",
            url: "/reciept/_View?path=" + "@(Model)"
        }).done(function (data) {
            $('#reciept-list').html(data);
        });
    }
</script>
}